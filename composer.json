{
    "name": "rdpascua/reporter",
    "description": "An elegant wrapper for JasperStarter using Laravel",
    "keywords": [
        "reporting",
        "jasperstarter",
        "jasper",
        "laravel"
    ],
    "homepage": "https://github.com/rdpascua/reporter",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Reporter\\": "src"
        }
    },
    "authors": [
       {
           "name": "Darick Pascua",
           "email": "rdpascua@outlook.com"
       }
    ],
    "require-dev": {
        "laravel/pint": "^1.7",
        "phpunit/phpunit": "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "pint": "./vendor/bin/pint"
    },
    "require": {
        "php": "^8.0",
        "symfony/process": "^6.0"
    },
     "extra": {
        "laravel": {
            "providers": [
                "Reporter\\ReporterServiceProvider"
            ],
            "aliases": {
                "Health": "Reporter\\Facades\\Reporter"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
